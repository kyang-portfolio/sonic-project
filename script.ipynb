    "openai.api_key = \"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\"",
